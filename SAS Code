*Subsetting to individuals with metabolic syndrome and diabetes; 
data MD;
	set Work.Complete;
	label MetsD = 'Metabolic syndrome and diabetes';
	if MetS = 'Yes' & Diabstatus = 1 then MetsD = 'Yes';
	else MetsD = 'No';
    
